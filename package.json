{
  "name": "@aibex/ayup",
  "version": "0.1.3",
  "description": "a(ny)yup. Useful Yup helpers and functions. Synchronous and Async.",
  "homepage": "https://github.com/aibexhq/ayup",
  "author": "Aibex <oss@aibex-corp.com>",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/aibexhq/ayup/issues"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "browser": "./cjs/index.js",
  "peerDependencies": {
    "yup": ">0.27.0 || ^0.28.0"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/runtime-corejs3": "^7.7.7",
    "core-js": "3.6.1",
    "promise.allsettled": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "7.7.7",
    "ava": "^2.4.0",
    "commitizen": "^4.0.3",
    "concurrently": "^5.0.2",
    "emoji-cz": "^0.3.0",
    "esm": "^3.2.25",
    "husky": "^4.0.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "release-it": "^12.4.3",
    "yup": "^0.28.0"
  },
  "keywords": [
    "yup",
    "validation",
    "helpers",
    "plugin",
    "joi"
  ],
  "browserslist": "last 2 versions, not dead, > 0.2%",
  "files": [
    "cjs/",
    "esm/"
  ],
  "scripts": {
    "build": "rm -rf cjs/* && babel esm --out-dir cjs",
    "bt": "yarn build && yarn test",
    "watch": "nodemon -L --ignore cjs/ --exec yarn bt",
    "test": "yarn build && CI=1 ava",
    "ci": "pretty-quick --check && yarn build && CI=1 ava",
    "release": "release-it",
    "____________________": "echo \"The following sub-commands are used to serve the main commands above\"",
    "format:prettier": "pretty-quick --staged",
    "husky:precommit": "concurrently \"npm:format:*\" && yarn build && git add -A && CI=1 ava",
    "commit:cz": "exec < /dev/tty && git-cz --hook || true"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run husky:precommit",
      "prepare-commit-msg": "if [ -t 1 ] ; then yarn run commit:cz; fi"
    }
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
